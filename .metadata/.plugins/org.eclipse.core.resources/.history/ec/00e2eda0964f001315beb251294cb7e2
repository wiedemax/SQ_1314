public class EmployedStudent implements EmployedStudentInterface{
	private StudentInterface _student;
	private EmployeeInterface _employee;
	
	public EmployedStudent(StudentInterface student, EmployeeInterface employee)
	{
		_student = student;
		_employee = employee;
	}
	
	public int getUnNumber() {
		return _student.getUnNumber();
	}
	public void setUnNumber(int unNumber) {
		_student.setUnNumber(unNumber);
	}
	public float getSalary() {
		return _employee.getSalary();
	}
	public void setSalary(float salary) {
		_employee.setSalary(salary);
	}
}
